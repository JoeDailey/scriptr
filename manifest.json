{
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "jquery.js", "main.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "description": "Run any script on any page! Be productive! Be mischievous! ",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png"
   },
   "manifest_version": 2,
   "name": "Scriptr",
   "permissions": [ "http://*/*", "https://*/*", "tabs" ],
   "version": "0.1.1"
}
